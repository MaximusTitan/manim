{
    "version": 2,
    "builds": [
      {
        "src": "api/index.py",
        "use": "@vercel/python"
      },
      {
        "src": "package.json",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "api/index.py"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "functions": {
      "python_runtime": "python3.10"
    }
  }
  